<h1 align="center">LLAW GNOS</h1>
<form method="post" action="/newsong">
  <div class="new_song">
    <h2 align="center">Submit a new song</h2>
    <h3>Artist:</h3><input type="text" name="artist" value="<%= @song.artist %>">
    <h3>Title:</h3><input type="text" name="title" value="<%= @song.title %>">
    <h3>URL:</h3><input type="text" name="url" value="<%= @song.url %>">
  </div>
  <div>
    <input type="submit" value="Submit">
  </div>

  <div class="song_wall">
    <ul>
    <% @songs.each do |song| %>
    <hr>
      <li>
        <h3><%= song.title %></h3>
        <h4><%= song.artist %></h4>
        <iframe src="<%= song.embed_url %>"></iframe>
      </li>
    <% end %>
  </ul>
  </div>
</form>
